{
    "manifest_version": 3,
    "name": "Proxy Toggle",
    "version": "1.0",
    "description": "Toggle proxy setting with a toolbar button",
    "permissions": ["proxy"],
    "icons": {
      "32": "icon.png"
    },
    "action": {
      "default_icon": "icon.png",
      "default_title": "Proxy Toggle"
    },
    "background": {
        "scripts": ["background.js"]
    }
  }